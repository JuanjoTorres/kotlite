
fun main (): None {
    var esRojo : Boolean = true;
    var esAzul : Boolean = false;

    if (esRojo) {
        print("Es rojo!");
    } else {
        print("No es rojo!");
    }

    if (!esAzul and !esRojo) {
        print("Es amarillo!");
    } else {
        print("Es rojo o azul!");
    }

    if (esAzul or esRojo) {
        print("Es un color del arcoiris!");
    } else {
        print("No es un color del arcoiris!");
    }

}
