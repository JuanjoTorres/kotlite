var verdadero: Boolean = true;
val falso: Boolean = false;

fun main (): None {

    print("Not verdadero: ", !true);
    print("Not falso: ", !false);
    print("OR true y true: ", true or false);
    print("OR true y false: ", true or false);
    print("OR false y false: ", true or false);
    print("AND true y true: ", true and false);
    print("AND true y false: ", true and false);
    print("AND false y false: ", true and false);

    if (!falso) {
        print("Not falso es true");
    } else {
        print("Error, no debería haber entrado aqui");
    }

    if (verdadero and (falso or verdadero)) {
        print("Verdadero AND (falso OR verdaero) es true");
    } else {
        print("Error 2, no debería haber entrado aqui");
    }
}