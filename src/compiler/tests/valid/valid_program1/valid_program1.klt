fun max(num1: Int; num2: Int): Int {

    var res: Int;

    if (num1 < num2) {
        res = num2;
    } else {
        res = num1;
    }

    return res;
}

fun min(num1: Int; num2: Int): Int {

    var res: Int;

    if (num1 > num2) {
        res = num2;
    } else {
        res = num1;
    }

    return res;
}

