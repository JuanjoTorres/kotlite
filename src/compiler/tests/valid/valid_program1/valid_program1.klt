val a: Int = 6;
var b: Int;

fun max(num1: Int; num2: Int): Int {

    var res: Int;

    if (num1 < num2) {
        res = num2;
    } else {
        res = num1;
    }

    return res;
}

fun main (a: Int; b: Int) : None {
    var c: Int;
    var asa: Int;
    var d : Int;

    b = 10;
    c = 30 + a * 3;
    d = c / 3;
    asa = max(b, d);

    print("El numero mas grande es... %d.", asa);
}
