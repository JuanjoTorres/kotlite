val num33 : Int = 33;

fun getNum(): Int {
    print("El numero es: %d", num33);
    return num33;
}

fun getNum2(): Int {
    var numero : Int = 3;
    numero = numero + 3;
    return numero;
}

fun main (): None {
    var numero: Int;

    numero = getNum();
    print("El resultado es: %d", numero);

    numero = getNum2();
    print("El resultado del segundo numero es: %d", numero);
}